        <CheckBox Content="12:00" HorizontalAlignment="Left" Margin="47,89,0,-104" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2"/>
        <CheckBox Content="12:00" HorizontalAlignment="Left" Margin="100,89,0,-104" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2" Grid.ColumnSpan="2"/>
