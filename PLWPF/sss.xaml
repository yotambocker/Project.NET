        <CheckBox Content="10:00" HorizontalAlignment="Left" Margin="28,49,0,-64" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2" Grid.Column="1"/>
