        <CheckBox Content="11:00" HorizontalAlignment="Left" Margin="100,69,0,-84" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2" Grid.ColumnSpan="2"/>
        <CheckBox Content="11:00" HorizontalAlignment="Left" Margin="81,69,-3,-84" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2" Grid.Column="1"/>
        <CheckBox Content="12:00" HorizontalAlignment="Left" Margin="81,89,-3,-104" Grid.Row="20" VerticalAlignment="Top" Checked="CheckBox_Checked" RenderTransformOrigin="2.524,2.2" Grid.Column="1"/>
