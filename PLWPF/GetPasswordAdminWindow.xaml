<Window x:Class="PLWPF.GetPasswordAdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
       Title="GetPasswordAdminWindow" Height="120.854" Width="245.732">
    <Grid Margin="0,0,1.2,-14.4" Keyboard.KeyUp="Grid_KeyUp" >
        <PasswordBox HorizontalAlignment="Left" x:Name="checkPasswordText" Margin="10,41,0,0" VerticalAlignment="Top" Height="17" Width="136" RenderTransformOrigin="0.449,-1.356"/>
        <Label Content="Please enter a password" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="136"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="166,41,0,0" VerticalAlignment="Top" Width="60" Height="26" Click=" Check_Click"/>
        
    </Grid>
</Window>
